-- with "florist";
project ada_pjsua_test is

   for Main use ("ada_pjsua_test");
   for Source_Dirs use ("src","src/pjsua","src/pjsip","src/pj");
   for Object_Dir use "build";
   for Exec_Dir use ".";


   package Builder is
      for Default_Switches ("Ada") use ("-gnat05", "-c", "-b");
   end Builder;

   package Compiler is
      for Default_Switches ("Ada") use ("-g");
   end Compiler;

   -- Linking will be done externally

   package Binder is
      for Default_Switches ("Ada") use ("-g");
   end Binder;

end ada_pjsua_test;

